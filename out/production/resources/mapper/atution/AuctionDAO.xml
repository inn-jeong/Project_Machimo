<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.project_machimo.auction.dao.AuctionDAO">
    <update id="highestBidUpdate">
        update AUCTIONS set HIGHEST_BID = #{param1}
        where products_id = #{param2}
    </update>
    <select id="pList" resultType="com.example.project_machimo.auction.dto.AuctionVO">
        select * from auctions where products_id = #{0}
    </select>
    <select id="endList" resultType="com.example.project_machimo.auction.dto.AuctionVO">
        select * from auctions  where end_time <![CDATA[<=]]> #{param1}
    </select>

</mapper>